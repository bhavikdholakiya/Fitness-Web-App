<body>
    <%- include ('../partials/navbar.ejs') %>
    <section id="set-admin">
        <div id="set-add">
            <h2 style="width: max-content;margin: auto;font-size: 2em;">Create Set</h2>
            <!-- <div class="container">
                <label><h1>Sample Multi Data Select Box</h1></label>
                <br/><br/>
                  <select name="" multiple id="addiction">
                      <% arr.forEach(item=>{ %>
                        <option><%= item.name %></option>
                      <% }) %>
                  </select>
              </div>
            <div class="slider">
            <input type = "range" id="strength" min="0" max="100" onchange="rangevalue.value=value"/>
            <output id="rangevalue">50</output>
            </div> -->
            <input type="number" name="time" id="time">
            <div class="custom-select" style="width:200px;">
                <select id="bmi">
                  <option value="0">BMI</option>
                  <option value="0-17">0-17</option>
                  <option value="18-25">18-25</option>
                  <option value="26-50">26-50</option>
                </select>
              </div>

              <div class="custom-select" style="width:200px;">
                <select id="bod">
                  <option value="0">BOD</option>
                  <option value="1-4">1-4</option>
                  <option value="5-8">5-8</option>
                </select>
              </div>

              <div class="custom-select" style="width:200px;">
                <select id="WorkoutTime">
                  <option value="0">WorkoutTime</option>
                  <option value="0-3">0-3</option>
                  <option value="4-6">4-6</option>
                  <option value="7-9">7-9</option>
                  <option value="10-12">10-12</option>
                </select>
              </div>

              <div class="custom-select" style="width:200px;">
                <select id="Addiction">
                  <option value="0">Addiction</option>
                  <option value="1">Yes</option>
                  <option value="0">No</option>
                </select>
              </div>

              <div class="custom-select" style="width:200px;">
                <select id="Cholesterol">
                  <option value="0">Cholesterol</option>
                  <option value="100-150">100-150</option>
                  <option value="150-200">150-200</option>
                </select>
              </div>

              <div class="custom-select" style="width:200px;">
                <select id="BloodPressure">
                  <option value="0">Blood Pressure</option>
                  <option value="0-50">0-50</option>
                  <option value="51-80">51-80</option>
                </select>
              </div>
            <svg onclick="postSet()" class="app-sidebar-link" style="border:none;cursor:pointer;margin:10px auto 10px  auto"xmlns="http://www.w3.org/2000/svg" width="50" height="50" fill="currentColor" class="bi bi-arrow-right-circle" viewBox="0 0 16 16">
                <path fill-rule="evenodd" d="M1 8a7 7 0 1 0 14 0A7 7 0 0 0 1 8zm15 0A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM4.5 7.5a.5.5 0 0 0 0 1h5.793l-2.147 2.146a.5.5 0 0 0 .708.708l3-3a.5.5 0 0 0 0-.708l-3-3a.5.5 0 1 0-.708.708L10.293 7.5H4.5z"/>
              </svg>
        </div>
        <div style="width: 80%;height: 3px;background-color: black;"></div>
        <div id="add-activity">
            <h2 style="width: max-content;font-size: 2em;">Create Activity</h2>
            <input type="text" class="st-input" name="name" id="activity-name" placeholder="Activity Name:">
            <input type="text" class="st-input" style="height: 120px;" name="description" id="activity-description" placeholder="Activity Description:">
            <form id="video-form" action="/docter/uploadVideo" method="POST" enctype="multipart/form-data">
                <input type="file" name="video" id="video-input">
                <label for="video-input">Select Video To Upload</label>
                <input type="submit" value="Upload">
            </form>
            
              <input type="number" class="st-input" name="count" id="count" placeholder="Count:">
              <input type="number" class="st-input" name="time" placeholder="Time:" id="time">
              <input type="number" class="st-input" name="point" id="point" placeholder="Points:">
              <div id="replacingloader">
                <svg onclick="postActivity()" class="app-sidebar-link" style="border:none;cursor:pointer;margin:10px auto 10px  auto"xmlns="http://www.w3.org/2000/svg" width="50" height="50" fill="currentColor" class="bi bi-arrow-right-circle" viewBox="0 0 16 16">
                    <path fill-rule="evenodd" d="M1 8a7 7 0 1 0 14 0A7 7 0 0 0 1 8zm15 0A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM4.5 7.5a.5.5 0 0 0 0 1h5.793l-2.147 2.146a.5.5 0 0 0 .708.708l3-3a.5.5 0 0 0 0-.708l-3-3a.5.5 0 1 0-.708.708L10.293 7.5H4.5z"/>
                  </svg>
              </div>
              

        </div>

    </section>
    <div class="projects-section" id="total-activities" style="overflow: hidden;">
        <div class="projects-section-header">
            <p>Total Activities</p>
        </div>
        <div class="search-wrapper">
            <input class="search-input" id="search-input" type="text" placeholder="Search">
            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" stroke="currentColor"
                stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="feather feather-search"
                viewBox="0 0 24 24">
                <defs></defs>
                <circle cx="11" cy="11" r="8"></circle>
                <path d="M21 21l-4.35-4.35"></path>
            </svg>
        </div>
        <div class="projects-section-line">
            <div class="projects-status">
                <div class="item-status">
                        <!-- <span class="status-number">62</span>
                        <span class="status-type">Total Projects</span> -->
                    
                </div>
            </div>

        </div>
        <div class="project-boxes jsGridView" id="project-boxes">

        </div>
        
    </div>
    <div class="pagination">
          <svg onclick="prevPage()" xmlns="http://www.w3.org/2000/svg" width="40" height="40" fill="currentColor" class="bi bi-arrow-left-circle-fill" viewBox="0 0 16 16">
            <path d="M8 0a8 8 0 1 0 0 16A8 8 0 0 0 8 0zm3.5 7.5a.5.5 0 0 1 0 1H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H11.5z"/>
          </svg>
          <svg onclick="nextPage()" xmlns="http://www.w3.org/2000/svg" width="40" height="40" fill="currentColor" class="bi bi-arrow-right-circle-fill" viewBox="0 0 16 16">
            <path d="M8 0a8 8 0 1 1 0 16A8 8 0 0 1 8 0zM4.5 7.5a.5.5 0 0 0 0 1h5.793l-2.147 2.146a.5.5 0 0 0 .708.708l3-3a.5.5 0 0 0 0-.708l-3-3a.5.5 0 1 0-.708.708L10.293 7.5H4.5z"/>
          </svg>
    </div>
    <div class="projects-section" id="selectedAc" style="overflow: hidden;">
        <div class="projects-section-header">
            <p>Selected Activities</p>
        </div>
        <div class="search-wrapper">
            <input class="search-input" id="search-input" type="text" placeholder="Search">
            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" stroke="currentColor"
                stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="feather feather-search"
                viewBox="0 0 24 24">
                <defs></defs>
                <circle cx="11" cy="11" r="8"></circle>
                <path d="M21 21l-4.35-4.35"></path>
            </svg>
        </div>
        <div class="projects-section-line">
            <div class="projects-status">
                <div class="item-status">
                        <!-- <span class="status-number">62</span>
                        <span class="status-type">Total Projects</span> -->
                    
                </div>
            </div>

        </div>
        <div class="project-boxes jsGridView" id="project-boxes">

        </div>
        
    </div>
